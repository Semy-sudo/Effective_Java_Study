package item44;

public class TestLamda {

	public static void main(String[] args) {
		//1. Runnable
		Runnable runnable = () -> System.out.println("running!!!");//람다로 구현
		runnable.run();
		
		Runnable runnable2 = new Runnable(){
			@Override
			public void run() {
				System.out.println("running2!!!");
			};
		};
		runnable2.run();

	}

}
